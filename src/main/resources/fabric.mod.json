{
  "schemaVersion": 1,
  "id": "strawdummy",
  "version": "${version}",

  "name": "Straw Dummy",
  "description": "Added dummy players for you to test damage!",
  "authors": [
    "GabrielOlvH"
  ],
  "contact": {
    "homepage": "https://github.com/GabrielOlvH/Straw-Dummy",
    "sources": "https://github.com/GabrielOlvH/Straw-Dummy"
  },

  "license": "Apache-2.0",
  "icon": "assets/strawdummy/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "me.steven.strawdummy.StrawDummy"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "me.steven.strawdummy.StrawDummyClient"
      }
    ]
  },
  "mixins": [
    "strawdummy.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric-api": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "1.20.x"
  }
}
